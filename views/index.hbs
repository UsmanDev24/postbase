<ul class="p-8 list w-full max-w-2xl m-auto bg-base-100 rounded-box shadow-2xl">
  {{#each notelist}}
    <li class="list-row bg-base-200">
      <div>
        <a
          class="text-xl p-1 text-primary "
          href="/notes/view?key={{key}}"
        >{{title}}</a>
      </div>
      
        <a class=" tooltip tooltip-bottom  ml-auto text-secondary " data-tip=" Edit " href="/notes/edit?key={{key}}"><span data-feather="edit"></span></a>
      <a  class=" tooltip  tooltip-warning text-warning" data-tip=" Destroy " href="/notes/destroy?key={{key}}"><span data-feather="delete"></span></a>
    </li>
  {{/each}}
</ul>